{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "ll",
	"scopeName": "source.ll",
	"fileTypes": ["ll"],
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#comments"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.ll",
				"match": "\\b(if|while|for|return|break)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.ll",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.ll",
					"match": "\\\\."
				}
			]
		},
		"comments": {
			"patterns": [
				{
				"name": "comment.block.robot",
				"begin": "/\\*",
				"end": "\\*/",
				"patterns": [
					{
					"name": "comment.block.robot",
					"match": "."
					}
				]
				},
				{
				"name": "comment.line.double-slash.robot",
				"match": "//.*$"
				},
				{
				"name": "string.quoted.double.robot",
				"match": "\"(?:\\\\.|[^\\\\\"])*\""
				},
				{
				"name": "string.quoted.single.robot",
				"match": "'(?:\\\\.|[^\\\\'])*'"
				},
				{
				"name": "punctuation.bracket.curly.robot",
				"match": "[{}]"
				},
				{
				"name": "punctuation.bracket.square.robot",
				"match": "[\\[\\]]"
				},
				{
				"name": "punctuation.bracket.round.robot",
				"match": "[()]"
				}
			]
		}
	}
}