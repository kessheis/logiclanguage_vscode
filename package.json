{
  "publisher": "kesshei",
  "name": "logiclang",
  "displayName": "ll",
  "description": "Logic programming language 一个支持逻辑语言的编程语言",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "homepage": "https://github.com/kessheis/logiclanguage#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/kessheis/logiclanguage"
  },
  "bugs": {
    "url": "hhttps://github.com/kessheis/logiclanguage/issues"
  },
  "icon": "assets/logo.png",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ll.helloWorld",
        "title": "Hello World"
      }
    ],
    "languages": [
      {
        "id": "ll",
        "aliases": [
          "ll",
          "LogicLanguage",
          "Logic"
        ],
        "extensions": [
          ".ll"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ll",
        "scopeName": "source.ll",
        "path": "./syntaxes/ll.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "ll",
        "path": "./snippets/operators.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.109.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}
